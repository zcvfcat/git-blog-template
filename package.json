{
  "name": "git-blog-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "env:local": "NEXT_NODE_ENV=local&&NODE_ENV=development",
    "env:dev": "NEXT_NODE_ENV=development&&NODE_ENV=development",
    "env:prod": "NEXT_NODE_ENV=production&&NODE_ENV=production",
    "local": "yarn env:local next dev",
    "dev": "yarn env:dev next dev --turbo --show-all",
    "prod": "yarn env:prod next dev",
    "build:local": "yarn env:local next build",
    "build:dev": "yarn env:dev next build",
    "build:prod": "yarn env:prod next build",
    "start:local": "yarn env:local next start",
    "start:dev": "yarn env:dev next start",
    "start:prod": "yarn env:prod next start",
    "lint": "next lint"
  },
  "dependencies": {
    "autoprefixer": "10.4.14",
    "jotai": "^2.0.4",
    "next": "13.3.1",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "18.2.0",
    "scss": "^0.2.4",
    "swr": "^2.1.3",
    "tailwindcss": "3.3.1"
  },
  "devDependencies": {
    "@code-convention/eslint-config-nextjs": "^1.0.4",
    "@code-convention/prettier-config": "^1.0.2",
    "@types/node": "18.16.0",
    "@types/react": "18.0.38",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.3.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.8",
    "sass": "^1.62.0",
    "typescript": "5.0.4",
    "typescript-plugin-css-modules": "^5.0.1",
    "yaml": "^2.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
